{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console":0,
        "max-len": ["error", { "code": 120 }],
        "require-await": 2,
        "no-await-in-loop": 2,
        "no-return-await": 2
    }
}
